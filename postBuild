mkdir /home/extensions
cp extensions_student /home/extensions

cd /home/extensions
jupyter nbextension install . --system
jupyter nbextension enable cell --system
jupyter nbextension enable codecell --system
jupyter nbextension enable control --system
jupyter nbextension enable debug --system
jupyter nbextension enable disablenotebookextensions --system
jupyter nbextension enable getattempts --system
jupyter nbextension enable getquestion --system
jupyter nbextension enable getsolutions --system
jupyter nbextension enable insertcell --system
jupyter nbextension enable jupyterevents --system
jupyter nbextension enable jupyterlogger --system
jupyter nbextension enable moveablecell --system
jupyter nbextension enable reloadsolutions --system
jupyter nbextension enable setcolorsandeditable --system
jupyter nbextension enable showhints --system
jupyter nbextension enable submit --system
jupyter nbextension enable submitattempt --system
jupyter nbextension enable submitcontainer --system
jupyter nbextension enable submithack --system
jupyter nbextension enable textcell --system
jupyter nbextension enable url --system
jupyter nbextension enable username --system

cp /studentfakefiles/tree.json /usr/local/etc/jupyter/nbconfig/
cp /studentfakefiles/notebook.json /usr/local/etc/jupyter/nbconfig/